# Homepage Widget API Keys
# These are optional - services will work without widgets but won't show detailed stats
# Create these in your 1Password vault under "Server" and reference them in the main .env

# Portainer API key (Settings -> Users -> API tokens)
HOMEPAGE_VAR_PORTAINER_KEY=

# Grafana service account token (Configuration -> Service accounts)
HOMEPAGE_VAR_GRAFANA_USER=
HOMEPAGE_VAR_GRAFANA_PASS=

# AdGuard Home credentials
HOMEPAGE_VAR_ADGUARD_USER=
HOMEPAGE_VAR_ADGUARD_PASS=

# MinIO credentials (root user)
HOMEPAGE_VAR_MINIO_KEY=
HOMEPAGE_VAR_MINIO_SECRET=

# Jellyfin API key (Dashboard -> Advanced -> API Keys)
HOMEPAGE_VAR_JELLYFIN_KEY=

# Tautulli API key (Settings -> Web Interface -> API Key)
HOMEPAGE_VAR_TAUTULLI_KEY=

# Prowlarr API key (Settings -> General -> API Key)
HOMEPAGE_VAR_PROWLARR_KEY=

# Sonarr API key (Settings -> General -> API Key)
HOMEPAGE_VAR_SONARR_KEY=

# Radarr API key (Settings -> General -> API Key)
HOMEPAGE_VAR_RADARR_KEY=

# qBittorrent credentials
HOMEPAGE_VAR_QBIT_USER=
HOMEPAGE_VAR_QBIT_PASS=

# Paperless-ngx token (Settings -> API Tokens)
HOMEPAGE_VAR_PAPERLESS_KEY=
